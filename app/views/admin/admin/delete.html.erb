<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="<%= asset_path "stylesheet.css"%>">
  </head>
  <body>
  <%= render :partial => "/layout/profile_header" %>
  <%= content_tag :div, flash.now[:notice], class: "notice" %>
  
  <%= form_tag("/admin/delete", method: "post") do %>  
    <%= select_tag :job_name, options_for_select(@jobs) %>
    <%= submit_tag("Delete") %>
  <% end %>


<%= render :partial => "/layout/profile_javascripts" %>
<%= render 'shared/footer' %>
  </body>
</html>